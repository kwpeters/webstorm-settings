<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="10" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="2500" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bLEFT OFF HERE\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bTODO\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bMUST\b.*">
      <option name="FOREGROUND" value="f5f4f5" />
      <option name="BACKGROUND" value="730000" />
    </pattern>
    <pattern case-sensitive="true" pattern="\bFUTURE\b.*" />
    <filter name="before checkin">
      <pattern index="2" />
      <pattern index="4" />
      <pattern index="0" />
      <pattern index="3" />
    </filter>
  </component>
</application>