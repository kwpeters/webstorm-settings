<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="11" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="2500" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="LEFT OFF HERE" />
    <pattern icon="important" case-sensitive="true" pattern="TODO" />
    <pattern icon="important" useCustomColors="true" pattern="\bmust\b.*">
      <option name="FOREGROUND" value="fffbee" />
      <option name="BACKGROUND" value="bf0f1e" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="bf0f1e" />
      <option name="ERROR_STRIPE_COLOR" value="bf0f1e" />
    </pattern>
    <pattern pattern="\bfuture\b.*" />
    <filter name="before checkin">
      <pattern index="3" />
      <pattern index="0" />
      <pattern index="4" />
      <pattern index="2" />
    </filter>
  </component>
</application>