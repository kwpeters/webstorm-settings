<templateSet group="kwpJavaScript">
  <template name="f" value="function $name$($params$) {$END$}$semicolon$" description="function" toReformat="false" toShortenFQNames="true">
    <variable name="semicolon" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="name" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="params" expression="" defaultValue="&quot;params&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="clog" value="console.log(&quot;$END$&quot;);" description="console.log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="strict" value="&quot;use strict&quot;;$END$" description="strict" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="jsdconstructor" value="/**&#10; * Constructs a new $classname$.&#10; *&#10; * @class&#10; * @classdesc class_description&#10; *&#10; * @param {type} parameter_name - parameter_description&#10; */" description="JSDoc for a constructor" toReformat="false" toShortenFQNames="true">
    <variable name="classname" expression="" defaultValue="Foo" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="jsdmethod" value="/**&#10; * method_descrption&#10; * @method&#10; * @param {parameter_type} parameter_name - parameter_description&#10; * @returns {return_type} return_description&#10; */" description="JSDoc for a method" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="jsdfunction" value="/**&#10; * function_descrption&#10; * @param {type} parameter_name - parameter_description&#10; * @returns {return_type} return_description&#10; */" description="JSDoc for a function" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="if" value="if ($END$) {&#10;    &#10;}" description="if" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
      <option name="TypeScript" value="true" />
