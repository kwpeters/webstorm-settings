<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Preferences...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'⌘,'</ShortCut>
          <Description>Preferences...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8' or 'F9'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Save All">
        <Item>
          <IdeaActionID>SaveAll</IdeaActionID>
          <ShortCut>'⌘S'</ShortCut>
          <Description>Save All</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>